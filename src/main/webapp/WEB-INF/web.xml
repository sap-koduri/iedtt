<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>UserRegistrationServlet</servlet-name>
  	<display-name>UserRegistrationServlet</display-name>
  	<description>User Registration</description>
  	<servlet-class>in.iedtt.controller.UserRegistrationServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UserLoginServlet</servlet-name>
  	<display-name>UserLoginServlet</display-name>
  	<description>Login Servlet</description>
  	<servlet-class>in.iedtt.controller.UserLoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CreateDefectServlet</servlet-name>
  	<display-name>CreateDefectServlet</display-name>
  	<description></description>
  	<servlet-class>in.iedtt.controller.CreateDefectServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UserRegistrationServlet</servlet-name>
  	<url-pattern>/UserRegistrationServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UserLoginServlet</servlet-name>
  	<url-pattern>/UserLoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CreateDefectServlet</servlet-name>
  	<url-pattern>/CreateDefectServlet</url-pattern>
  </servlet-mapping>
</web-app>
